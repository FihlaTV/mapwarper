<div id="container">

  <div id="left-col">
    <div id="video-intro" class="paraMargin">
      <div id="video-container">
        <iframe width="320" height="180" src="https://www.youtube.com/embed/z0n0O3miMWI" frameborder="0" allowfullscreen></iframe>
      </div>
      <h2><%= t('.overview_header') %></h2>
      <p><%= t('.overview_p1') %></p>
      <p><%= t('.overview_p2') %></p>
      <p><%= t('.overview_p3') %></p>
    </div>

  </div>
  <div id="right-col">
    <div id="search-form">
      <%= render :partial => 'shared/search' -%>
    </div>

    <div id="map_list">


      <% if @my_maps -%>
        <h3><%= t('common.my_maps_head') %></h3>
        <table id="map_table" cellpadding="3">
          <thead>
            <tr>
              <th><%= t('common.map_th') %></th>
              <th><%= t('common.title_th') %></th>
              <th><%= t('common.year_th') %></th>
              <th><%= t('common.last_th') %></th>
              <th><%= t('common.status_th') %></th>
            </tr>
          </thead>
          <%= render :partial => 'maps/map', :collection => @my_maps  unless @my_maps.nil? %>
        </table>
        <%= link_to t(".all_my_maps"), my_maps_path(:user_id => current_user), :class => "buttonWarper" -%>
      <%end -%>

    
      
     <h3><%= t('.unrect_maps') %></h3>

      <table id="map_table" cellpadding="3">
        <thead>
          <tr>
              <th><%= t('common.map_th') %></th>
              <th><%= t('common.title_th') %></th>
              <th><%= t('common.year_th') %></th>
              <th><%= t('common.last_th') %></th>
              <th><%= t('common.status_th') %></th>
          </tr>
        </thead>
        <%= render :partial => 'maps/map', :collection => @unwarped_maps unless @unwarped_maps.nil? %>
      </table>

      <%= link_to t(".see_maps"), maps_path, :class => "buttonWarper" %>

   

    </div>

  </div>
  <div class="clear"></div>
</div>
